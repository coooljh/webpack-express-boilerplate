{
  "name": "webpack-express-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate for running a Webpack workflow in Node express",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/christianalfoni/webpack-express-boilerplate.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node"
  ],
  "author": "Christian Alfoni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christianalfoni/webpack-express-boilerplate/issues"
  },
  "homepage": "https://github.com/christianalfoni/webpack-express-boilerplate",
  "scripts": {
    "test": "mocha",
    "start": "node server",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --colors",
    "start:prod": "cross-env NODE_ENV=production PORT=4000 node server",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "dependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.22.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-eslint": "^4.1.6",
    "babel-jscs": "^2.0.5",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "cross-env": "^1.0.7",
    "css-loader": "^0.19.0",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "html-webpack-plugin": "^1.6.1",
    "jscs": "^2.1.1",
    "jsdom": "^6.5.1",
    "mocha": "^3.2.0",
    "postcss-loader": "^1.3.0",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "style-loader": "^0.12.4"
  }
}
